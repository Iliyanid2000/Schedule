
public class employee 
{
int priority,-?[ DaysToWork;
String name;
int DaysToWork;
String[] availability;
	employee(int priority,String name, int DaysToWork, String[] availability)
	{
	this.priority = priority;
	this.DaysToWork = (DaysToWork > 7) ? 7 : DaysToWork;
	this.availability = (availability.length > 7) ? null : availability;
	this.name = name;	
	}
}
