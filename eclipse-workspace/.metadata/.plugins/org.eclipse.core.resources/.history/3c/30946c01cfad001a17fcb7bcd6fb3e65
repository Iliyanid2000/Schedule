
public class employee 
{
int priority;
int[] DaysToWork;
String name;
char[] availability;
char doubles;
	employee(int priority,String name, int[] DaysToWork, char[] availability,char doubles)
	{
	this.priority = priority;
	this.DaysToWork = (DaysToWork.length > 7) ? null : DaysToWork;
	this.availability = (availability.length > 7) ? null : availability;
	this.name = name;	
	this.doubles = (doubles == 'y' || doubles == 'n') ? doubles : 'n';
	}
	
}
